/*
 * Test class that demonstrates polymorphism through an interface.
 * 
 */


public class CowTester {
  
  public static void main(String[] args) {
    Cow john = new Cow(2);
      Cow mark = new Cow(3);
    System.out.println(john.compareTo(mark));
    System.out.println(mark.compareTo(john));
    System.out.println(mark.compareTo(mark));
  }
}
